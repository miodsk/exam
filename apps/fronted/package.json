{
  "name": "fronted",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "format": "prettier --write --experimental-cli src/",
    "gen:user": "swagger-typescript-api -p http://localhost:3001/api-json -o ./src/api -n userApi.ts --axios --instance-path @/utils/request",
    "gen:exam": "swagger-typescript-api -p http://localhost:3002/api-json -o ./src/api -n examApi.ts --axios --instance-path @/utils/request",
    "gen:api": "pnpm gen:user && pnpm gen:exam"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "axios": "^1.13.2",
    "element-plus": "^2.13.0",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "unplugin-icons": "^22.5.0",
    "vite-plugin-inspect": "^11.3.3",
    "vue": "^3.5.26",
    "vue-draggable-plus": "^0.6.0",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.7.4",
    "sass": "^1.97.2",
    "swagger-typescript-api": "^13.2.16",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.3.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.2.1"
  }
}
