      "entryFile": "index",
      "sourceRoot": "libs/entity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entity/tsconfig.lib.json"
      }
    },
    "exam": {
      "type": "application",
      "root": "apps/exam",
      "entryFile": "main",
      "sourceRoot": "apps/exam/src",
      "compilerOptions": {
        "tsConfigPath": "apps/exam/tsconfig.app.json"
      }
    },
    "exam-system": {
      "type": "application",
      "root": "apps/exam-system",
      "entryFile": "main",
      "sourceRoot": "apps/exam-system/src",
