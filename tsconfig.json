{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "resolvePackageJsonExports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/auth": [
        "libs/auth/src"
      ],
      "@app/auth/*": [
        "libs/auth/src/*"
      ],
      "@app/database": [
        "libs/database/src"
      ],
      "@app/database/*": [
        "libs/database/src/*"
      ],
      "@app/decorator": [
        "libs/decorator/src"
      ],
      "@app/decorator/*": [
        "libs/decorator/src/*"
      ],
      "@app/dto": [
        "libs/dto/src"
      ],
      "@app/dto/*": [
        "libs/dto/src/*"
      ],
      "@app/email": [
        "libs/email/src"
      ],
      "@app/email/*": [
        "libs/email/src/*"
      ],
      "@app/entity": [
        "libs/entity/src"
      ],
      "@app/entity/*": [
        "libs/entity/src/*"
      ],
      "@app/excel": [
        "libs/excel/src"
      ],
      "@app/excel/*": [
        "libs/excel/src/*"
      ],
      "@app/interceptor": [
        "libs/interceptor/src"
      ],
      "@app/interceptor/*": [
        "libs/interceptor/src/*"
      ],
      "@app/prisma": [
        "libs/prisma/src"
      ],
      "@app/prisma/*": [
        "libs/prisma/src/*"
      ],
      "@app/redis": [
        "libs/redis/src"
      ],
      "@app/redis/*": [
        "libs/redis/src/*"
      ],
      "@app/types": [
        "libs/types/src"
      ],
      "@app/types/*": [
        "libs/types/src/*"
      ],
      "@app/vo": [
        "libs/vo/src"
      ],
      "@app/vo/*": [
        "libs/vo/src/*"
      ]
    }
  }
}